      <processElements xsi:type="org.eclipse.epf.uma:WorkProductDescriptor" xmi:id="_NQoqodqzEeiE2vpKaXQomw" name="software" guid="_NQoqodqzEeiE2vpKaXQomw" presentationName="Software" briefDescription="Um conjunto consistente de produtos de software que incluem: &#xD;&#xA;▸ Especificação de requisitos&#xD;&#xA;▸ Componentes de Software &#xD;&#xA;▸ Software (unidade, produto, item) &#xD;&#xA;▸ Relatórios de teste e incidente &#xD;&#xA;▸ Manual operacional &#xD;&#xA;▸ Manual do usuário" isPlanned="false" superActivities="_mDk1QdqyEeiE2vpKaXQomw">
