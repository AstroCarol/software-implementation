          <contentElements xsi:type="org.eclipse.epf.uma:Role" xmi:id="_ocqHINqfEeiE2vpKaXQomw" name="cliente" guid="_ocqHINqfEeiE2vpKaXQomw" presentationName="Cliente" briefDescription="O cliente possui a capacidade de explicar os requisitos que lhe são desejáveis. Ele deve ter autoridade para aprovar ou reprovar requisitos e suas alterações. No ramo cliente estão inclusos representantes dos usuários, para assim garantir que o ambiente operacional seja abordado. Possui conhecimento e experiência no domínio da aplicação.">
            <methodElementProperty xmi:id="_Fw4PINqgEeiE2vpKaXQomw" name="me_edited" value="true"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Role" xmi:id="_GmZh4NqgEeiE2vpKaXQomw" name="gerente" guid="_GmZh4NqgEeiE2vpKaXQomw" presentationName="Gerente" briefDescription="Possui capacidade de liderança, com experiência em tomada de decisões, planejamento, gerenciamento de pessoal, delegação e supervisão, finanças e desenvolvimento de software">
            <methodElementProperty xmi:id="_Q2eDgNqgEeiE2vpKaXQomw" name="me_edited" value="true"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Role" xmi:id="_ToLWINqgEeiE2vpKaXQomw" name="equipe" guid="_ToLWINqgEeiE2vpKaXQomw" presentationName="Equipe" briefDescription="Possui conhecimento e experiência de acordo com seus papéis no projeto.">
            <methodElementProperty xmi:id="_cQqO4NqgEeiE2vpKaXQomw" name="me_edited" value="true"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Task" xmi:id="_idOe0NqgEeiE2vpKaXQomw" name="revisao_do_plano_de_projeto" guid="_idOe0NqgEeiE2vpKaXQomw" presentationName="Revisão do Plano de Projeto" briefDescription="O gerente deve realizar uma reunião com todos os participantes para garantir o entendimento completo do projeto e seus objetivos. O gerente deve ter certeza de que todos os membros da equipe foram convocados para a reunião. No final da reunião, o gerente verificar se toda a equipe de trabalho entendeu e aceitou o envolvimento no projeto." performedBy="_ToLWINqgEeiE2vpKaXQomw _GmZh4NqgEeiE2vpKaXQomw" mandatoryInput="_lfcnwNqiEeiE2vpKaXQomw" output="_lfcnwNqiEeiE2vpKaXQomw">
            <methodElementProperty xmi:id="_yIJfkNqgEeiE2vpKaXQomw" name="me_edited" value="true"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Artifact" xmi:id="_M1CjgNqiEeiE2vpKaXQomw" name="documento_de_aceitacao" guid="_M1CjgNqiEeiE2vpKaXQomw" presentationName="Documento de Aceitação" briefDescription="Documento estabelecendo a aceitação do cliente das entregas estabelecidas no projeto.">
            <methodElementProperty xmi:id="_R5UVsNqiEeiE2vpKaXQomw" name="me_edited" value="true"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Artifact" xmi:id="_VG_LkNqiEeiE2vpKaXQomw" name="descricao_do_projeto" guid="_VG_LkNqiEeiE2vpKaXQomw" presentationName="Descrição do Projeto" briefDescription="Uma descrição de alto nível do projeto que inclui escopo, objetivos e principais entregáveis. ">
            <methodElementProperty xmi:id="_joBMENqiEeiE2vpKaXQomw" name="me_edited" value="true"/>
