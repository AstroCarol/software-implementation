          <contentElements xsi:type="org.eclipse.epf.uma:Artifact" xmi:id="_lfcnwNqiEeiE2vpKaXQomw" name="plano_de_projeto" guid="_lfcnwNqiEeiE2vpKaXQomw" presentationName="Plano de Projeto" briefDescription="Uma declaração de como e quando os objetivos de um projeto devem ser alcançados, mostrando os principais produtos, marcos, atividades e recursos necessários ao projeto.">
            <methodElementProperty xmi:id="_qBeogNqiEeiE2vpKaXQomw" name="me_edited" value="true"/>
          </contentElements>
          <contentElements xsi:type="org.eclipse.epf.uma:Artifact" xmi:id="_rD-nUNqiEeiE2vpKaXQomw" name="documento_de_requisitos" guid="_rD-nUNqiEeiE2vpKaXQomw" presentationName="Documento de Requisitos" briefDescription="Documento em que todos os requisitos identificados especificados.">
